using namespace std;
#include <iostream>
#include <string>
#include <vector>
void Reverse(vector<int>& v){
    for (int i = 0; i < v.size()/2; i++) {
        v[i] = v[v.size() - i];
    }
    
}
int main()
{
    vector<int> numbers = { 1, 5, 3, 4, 2 };
    Reverse(numbers);
    for (int i = 0; i < numbers.size(); i++) {
        cout << numbers[i] << " ";
    }
}
